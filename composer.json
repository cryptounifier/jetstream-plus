{
    "name": "cryptounifier/jetstream-plus",
    "description": "Some extra features to the default Laravel Jetstream + Socialstream stack.",
    "keywords": ["laravel", "jetstream", "socialstream", "jetstream-plus"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "cryptounifier",
            "email": "contact@cryptounifier.io"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/jetstream": "^4.0.0",
        "joelbutcher/socialstream": "^5.4.0",
        "intervention/image": "^3.0",
        "cryptounifier/helpers": "^0.3.0"
    },
    "autoload": {
        "psr-4": {
            "CryptoUnifier\\JetstreamPlus\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "stubs/app/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CryptoUnifier\\JetstreamPlus\\JetstreamPlusServiceProvider"
            ]
        }
    }
}
